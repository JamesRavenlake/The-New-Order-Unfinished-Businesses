defined_text = {
    name = GetHidingOurInterestsGUIPointAvailableJiangsu
    text = {
        trigger = {
                set_temp_variable = { temp1 = CHI_Hiding_Our_Interests_GUI_point_max_jiangsu }
                set_temp_variable = { temp2 = CHI_Hiding_Our_Interests_GUI_point_available_jiangsu }
                subtract_from_temp_variable = { temp1 = temp2 }
                set_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text = temp1 }
                check_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text = 1 }
        }
        localization_key = CHI_Hiding_Our_Interests_GUI_point_available_max_jiangsu_localization_key
    }
    text = {
        trigger = {
            set_temp_variable = { temp1 = CHI_Hiding_Our_Interests_GUI_point_max_jiangsu }
            set_temp_variable = { temp2 = CHI_Hiding_Our_Interests_GUI_point_available_jiangsu }
            subtract_from_temp_variable = { temp1 = temp2 }
            set_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text = temp1 }
            check_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text > 0.75 }
            OR = {
                check_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text < 1 }
            }
        }
        localization_key = CHI_Hiding_Our_Interests_GUI_point_available_high_jiangsu_localization_key
    }
    text = {
        trigger = {
            set_temp_variable = { temp1 = CHI_Hiding_Our_Interests_GUI_point_max_jiangsu }
            set_temp_variable = { temp2 = CHI_Hiding_Our_Interests_GUI_point_available_jiangsu }
            subtract_from_temp_variable = { temp1 = temp2 }
            set_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text = temp1 }
            check_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text > 0.50 }
            OR = {
                check_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text = 0.75 }
                check_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text < 0.75 }
            }
        }
        localization_key = CHI_Hiding_Our_Interests_GUI_point_available_slightly_high_jiangsu_localization_key
    }
    text = {
        trigger = {
            set_temp_variable = { temp1 = CHI_Hiding_Our_Interests_GUI_point_max_jiangsu }
            set_temp_variable = { temp2 = CHI_Hiding_Our_Interests_GUI_point_available_jiangsu }
            subtract_from_temp_variable = { temp1 = temp2 }
            set_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text = temp1 }
            check_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text > 0.25 }
            OR = {
                check_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text = 0.50 }
                check_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text < 0.50 }
            }
        }
        localization_key = CHI_Hiding_Our_Interests_GUI_point_available_middle_jiangsu_localization_key
    }
    text = {
        trigger = {
            set_temp_variable = { temp1 = CHI_Hiding_Our_Interests_GUI_point_max_jiangsu }
            set_temp_variable = { temp2 = CHI_Hiding_Our_Interests_GUI_point_available_jiangsu }
            subtract_from_temp_variable = { temp1 = temp2 }
            set_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text = temp1 }
            OR = {
                check_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text = 0.25 }
                check_variable = { CHI_Hiding_Our_Interests_GUI_point_available_text < 0.25 }
            }
        }
        localization_key = CHI_Hiding_Our_Interests_GUI_point_available_low_jiangsu_localization_key
    }
}
